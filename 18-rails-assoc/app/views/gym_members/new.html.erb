<!-- The form -->
<!-- POST /gym_members -->
<!--   # params = { gym_members: {name: "....", phone_number: "....", address: "...."}} -->
<%= form_for @gym_member do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.label :phone_number %>
  <%= f.text_field :phone_number %>
  <%= f.submit %>
<% end %>
